import java.util.ArrayList;

import main.NaveView;
import model.naveLogic;

public class NaveController  {
	
	
	PApplet app;
    int dir;
    boolean right, left ;
    
    
    
	public static void moverDerecha(PApplet app) {
		NaveView.posX+=5;
	}
	public static void moverIzquierda (PApplet app) {
		NaveView.posX-=5;
	}
	
	
 

		
	
    
	
		

	
	
	

}
