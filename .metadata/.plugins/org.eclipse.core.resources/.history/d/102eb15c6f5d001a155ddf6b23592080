package main;
import processing.core.PApplet;

public class NaveView {
		
	private int posX;
	private PApplet app;


	public NaveView (int posX,PApplet app) {
		this.posX=app.width/2;
		this.app=app;
		
	}
	

	public void pintar(PApplet app) {
		app.fill(255);
		app.rect(this.posX,app.height-20,20,20);
	}
	}


